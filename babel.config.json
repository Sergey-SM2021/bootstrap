{
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    [
      "i18next-extract",
      {
        "locales": ["ru", "en"],
        "keyAsDefaultValue": true
      }
    ],
    "@babel/plugin-transform-runtime",
    ["@babel/plugin-transform-typescript", { "isTSX": true }]
  ]
}
