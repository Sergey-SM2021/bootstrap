{
  "name": "bootstrap",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "backend": "node ./backend/index.js",
    "html-report": "reg-cli ./.loki/current ./.loki/reference ./.loki/difference -R ./report.html",
    "eslint": "eslint src/** --fix",
    "start": "webpack serve --env PORT=8081",
    "build:prod": "webpack --env MODE=production",
    "build:dev": "webpack --env MODE=development",
    "stylelint": "stylelint **/*.scss --fix",
    "test": "jest",
    "test:loki": "yarn loki --config ./config/storybook test",
    "loki:approve": "yarn loki --config ./config/storybook approve",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "jest-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "loki:ci": "storybook build -c ./config/storybook && loki --config ./config/storybook --requireReference --reactUri file:./storybook-static",
    "prepare": "husky install"
  },
  "keywords": [],
